<div class="list-group">
	{% for user in users %}
	<a href="#" class="list-group-item">
		 <h4 class="list-group-item-heading">{{ user.username }}</h4>
		 <p class="list-group-item-text">
		 	{% for vote in user.voteArticles %}
		 	{{ vote.article.nom }} : {{ vote.note }}<br />
		 	{% endfor %}
		 </p>
	</a>
	{% endfor %}
</div>